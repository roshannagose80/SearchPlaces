# platform :ios, '9.0'

target 'SearchPlaces' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'GoogleMaps'
  # Pods for SearchPlaces

  target 'SearchPlacesTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SearchPlacesUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
